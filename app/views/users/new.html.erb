<%= form_for @user do |f| %>
	<%=f.label :fname %>
	<%=f.text_field :fname %>

	<%=f.label :lname %>
	<%=f.text_field :lname %>

	<%=f.label :balance %>
	<%=f.number_field :balance %>

	<%=f.label :email %>
	<%=f.text_field :email %>

	<%=f.label :password %>
	<%=f.password_field :password %>
	<%=f.submit :create %>
	<% end %>